{
  "name": "firestation-electron",
  "productName": "Firestation",
  "description": "Starter for your Electron application",
  "version": "0.2.0",
  "private": true,
  "author": "mrjoeroddy@gmail.com",
  "homepage": "https://www.firestation.io",
  "main": "app/background.js",
  "build": {
    "appId": "com.example.electron-boilerplate",
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "publish": null
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "preunit": "webpack --config=build/webpack.unit.config.js --env=test --display=none",
    "unit": "electron-mocha temp/specs.js --renderer --require source-map-support/register",
    "pree2e": "webpack --config=build/webpack.app.config.js --env=test --display=none && webpack --config=build/webpack.e2e.config.js --env=test --display=none",
    "e2e": "mocha temp/e2e.js --require source-map-support/register",
    "test": "npm run unit && npm run e2e",
    "start": "node build/start.js",
    "release": "npm test && webpack --config=build/webpack.app.config.js --env=production && electron-builder"
  },
  "dependencies": {
    "bootstrap": "^4.0.0",
    "classnames": "^2.2.5",
    "firebase-admin": "^5.11.0",
    "fs-jetpack": "^1.0.0",
    "jquery": "^3.3.1",
    "mobx": "^4.1.1",
    "mobx-react": "^5.0.0",
    "moment": "^2.21.0",
    "popper.js": "^1.14.1",
    "react": "^16.3.0",
    "react-ace": "^5.10.0",
    "react-dom": "^16.3.0",
    "react-tooltip": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.5",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.42",
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.5",
    "@babel/preset-react": "^7.0.0-beta.42",
    "babel-loader": "^8.0.0-beta.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^7.0.0-beta.3",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.0",
    "css-loader": "^0.28.7",
    "electron": "^1.8.2-beta5",
    "electron-builder": "^19.43.3",
    "electron-devtools-installer": "^2.2.3",
    "electron-mocha": "^5.0.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "mocha": "^4.0.1",
    "node-sass": "^4.8.3",
    "sass-loader": "^6.0.7",
    "source-map-support": "^0.5.0",
    "spectron": "^3.7.2",
    "style-loader": "^0.19.0",
    "webpack": "^3.11.0",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.6.0"
  }
}
